#include <iostream>
#include<string>
#include<algorithm>


using namespace std;
int factorial(int n);


int main()
{
int n,final_fact;
cin>>n;
final_fact = factorial(n);
cout <<final_fact;

}
int factorial(int n)
{
    int total_fact;
    if (n==1)
    {
        return 1;
    }
    else if (n==0)
    {
        return 1;
    }
    else
    {
      total_fact = n*factorial(n-1) ;
      return total_fact;
    }



}
