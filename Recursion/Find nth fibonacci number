#include <iostream>
#include<string>
#include<algorithm>


using namespace std;
int fibonnaci(int n);


int main()
{
int n,ans;
cin>>n;

ans = fibonnaci(n);
cout <<ans;

}
int fibonnaci(int n)
{
    int total;
    if (n==2)
    {
        return 1;
    }
    else if (n==1)
    {
        return 0;
    }
    else
    {
      total = fibonnaci( n-1)+fibonnaci( n-2);
      return total;

    }



}
