#include <iostream>
#include<string>
#include<algorithm>


using namespace std;

void arr(int n,int a[],int p);

int main()
{
int n,a[10];
cin>>n;

int i;
for(i=0;i<n;i++)
{
    cin>>a[i];
}
int p=n;
arr( n,a,p);
}
void arr(int n,int a[],int p)
{
   if (n==p||n==1)
   {
       cout<<a[0]<<"\n";
   }
   arr(n-1,a+1,p);



}
