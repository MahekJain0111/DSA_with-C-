#include <iostream>
#include<string>
#include<algorithm>


using namespace std;

int print_inc(int n);

int main()
{



int n;
    cin>>n;
    cout<<print_inc(n);

}
int print_inc(int n)
{
    if (n==1)
    {
        return n;
    }
    else
    {
       cout<<print_inc(n-1) ;
       return n;
    }
}
